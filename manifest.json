{
  "manifest_version": 2,
  "name": "Webscout Parser",
  "description": "Parses source search results and throws data back to webscout",
  "version": "1.0",
  "content_scripts": [
    {"matches": ["https://45.55.18.141/*", "http://localhost:8888/*"], "include_globs": ["*#/orders*"], "js": ["bower_components/lodash/lodash.js", "bower_components/jquery/dist/jquery.js", "webscout_orders_page/webscout_orders_page_controller.js"]}
  ],
  "background": {
    "scripts": ["bower_components/jquery/dist/jquery.js", "background.js"]
  },
  "web_accessible_resources": [
    "bower_components/jquery/dist/jquery.js"
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}